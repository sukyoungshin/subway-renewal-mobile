{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/index.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/_assets/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/dist/client/_assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server/index.cjs"
    }
  ]
}
