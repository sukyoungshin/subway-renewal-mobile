{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/index.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/dist/client/index.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/client/assets/$1"
    },
    {
      "src": "/search(\\.html)?",
      "dest": "/dist/client/search.html"
    },
    {
      "src": "/manifest.json",
      "dest": "/dist/client/manifest.json"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp|json))",
      "dest": "/dist/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server/index.cjs"
    }
  ]
}
