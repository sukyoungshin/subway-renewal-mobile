{
  "name": "subway",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "compression": "^1.8.1",
    "dotenv": "^17.2.1",
    "express": "4",
    "node-fetch": "^3.3.2",
    "path-to-regexp": "6.2.1",
    "react": "^19.1.1",
    "react-daum-postcode": "^3.0.1",
    "react-dom": "^19.1.1",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "6.26.2",
    "serve-static": "^2.2.0"
  },
  "scripts": {
    "dev:client": "vite",
    "dev:server": "tsx --env-file=.env server/index.ts",
    "build:client": "vite build",
    "build:server": "SSR_BUILD=true vite build --ssr",
    "build": "yarn build:client && yarn build:server",
    "start": "NODE_ENV=production tsx server/index.ts",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "format": "prettier .",
    "sync:build": "tsc -p scripts/tsconfig.scripts.json",
    "sync:run": "node dist-scripts/sync-issue-to-notion.cjs",
    "sync": "yarn sync:build && mv dist-scripts/sync-issue-to-notion.js dist-scripts/sync-issue-to-notion.cjs && yarn sync:run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react-daum-postcode/react": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.0.0",
    "terser": "^5.43.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.2"
  }
